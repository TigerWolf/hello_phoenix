<h2>Show log</h2>

<ul>

  <li>
    <strong>Activity:</strong>
    <%= @log.activity.name %>
  </li>

  <li>
    <strong>Amount:</strong>
    <%= @log.amount %>
  </li>

  <li>
    <strong>User:</strong>
    <%= @log.user.email %>
  </li>

</ul>

<%= link "Edit", to: log_path(@conn, :edit, @log) %>
<%= link "Back", to: log_path(@conn, :index) %>
